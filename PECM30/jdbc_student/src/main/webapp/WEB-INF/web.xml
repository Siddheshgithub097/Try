<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<context-param>
		<param-name>School</param-name>
		<param-value>Modern</param-value>
	</context-param>
	<servlet>
		<servlet-name>pankaj</servlet-name>
		<servlet-class>jdbc_student.SignUpController</servlet-class>
		<init-param>
			<param-name>onetime</param-name>
			<param-value>100000</param-value>
		</init-param>
		<init-param>
			<param-name>ins</param-name>
			<param-value>12000</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>pankaj</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>
	
	<servlet>
  <servlet-name>ram</servlet-name>
  <servlet-class>jdbc_student.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ram</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>
