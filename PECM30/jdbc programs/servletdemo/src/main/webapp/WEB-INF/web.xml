<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  <servlet-name>A</servlet-name>
  <servlet-class>servletdemo.Servlet1</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  <servlet-name>A</servlet-name>
  <url-pattern>/s1</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>B</servlet-name>
     <servlet-class>servletdemo.Servlet2</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>B</servlet-name>
  <url-pattern>/s2</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>N</servlet-name>
  <servlet-class>servletdemo.ServletCofig</servlet-class>
  <init-param>
   <param-name>name1</param-name>
   <param-value>akshay</param-value>
  </init-param>
  
  </servlet>
  
  <servlet-mapping>
  <servlet-name>N</servlet-name>
  <url-pattern>/configrandtext</url-pattern>
  </servlet-mapping>
  
  <context-param>
  <param-name>name</param-name>
  <param-value>ajay</param-value>
  </context-param>
</web-app>
